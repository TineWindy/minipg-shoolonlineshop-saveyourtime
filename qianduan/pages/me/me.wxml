<!--miniprogram/pages/me/me.wxml-->
<view class="userinfo">
  <view class="userinfo-avatar">
    <open-data type="userAvatarUrl"></open-data>
  </view>
  <view class="userinfo-nickname">
    <open-data type="userNickName"></open-data>
  </view>
</view>

<view>

  <wux-grids>
    <wux-grid thumb="http://pbqg2m54r.bkt.clouddn.com/logo.png" label="我的信息" bindtap="jump2person"/>
    <wux-grid thumb="http://pbqg2m54r.bkt.clouddn.com/logo.png" label="我买到的"/>
    <wux-grid thumb="http://pbqg2m54r.bkt.clouddn.com/logo.png" label="我卖出的"/>
    <wux-grid thumb="http://pbqg2m54r.bkt.clouddn.com/logo.png" label="关于拾樱铺"/>
  </wux-grids>
</view>
